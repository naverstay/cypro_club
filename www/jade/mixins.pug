mixin loop_while(counter)
    -i=0;
    while i < counter
        -i++;
        block

mixin header()
    div.header
        a.mob_menu.mobMenuBtn.i-menu(href='#')
        .section_inner.clearfix
            .logo_block
                a.logo(href='index.html')
                    img(src='i/logo.png')
                    span Традиции и  <br>  надежность

            .header_callback
                ul.share_block
                    li: a.sh_link.i-facebook(href='#')
                    li: a.sh_link.i-instagram(href='#')
                
                ul.phones_block
                    li Украина +38 (097) 17 11 844
                    li Кипр +35 799 69 55 80
                    li
                        a.phones_callback_btn.green_btn_2(href='#')
                            span заказать звонок
                        
            ul.nav_menu
                li.nav_item
                    a.nav_link(href='about.html') О нас
                li.nav_item
                    a.nav_link(href='about.html') Аренда
                li.nav_item
                    a.nav_link(href='about.html') Семейный отдых
                li.nav_item
                    a.nav_link(href='about.html') Дополнительные услуги
                li.nav_item
                    a.nav_link(href='about.html') Продажа
                li.nav_item
                    a.nav_link(href='about.html') Контакты
                

mixin popup()
    #callback_popup.popup(style='display:none;')
        form.validateMe(action='#')
            p Введите ваше имя и телефон и получите  <br>  развернутую консультацию специалиста 
            .form_cell.f_cell_v1
                input.f_input_v1(placeholder='Введите Ваше имя' class='validate[required]')
            .form_cell.f_cell_v1
                input.f_input_v1(placeholder='Введите Ваш телефон' class='validate[required]')
            .form_cell.f_cell_v1
                input.f_input_v1(placeholder='Введите Ваш e-mail')

            .form_cell.f_cell_v1
                button.btn_v2.green_btn.subscribe_btn Получить консультацию специалиста

